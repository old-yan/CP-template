			"    struct OpAdd{",
			"        template<class T>T operator()(T x,T y){return x+y;}",
			"    };",
			"    struct OpSet{",
			"        template<class T>T operator()(T x,T y){return y;}",
			"    };",
			"    struct OpMax{",
			"        template<class T>T operator()(T x,T y){return x>y?x:y;}",
			"    };",
			"    struct OpMin{",
			"        template<class T>T operator()(T x,T y){return x<y?x:y;}",
			"    };",
